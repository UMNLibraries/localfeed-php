{
  "name": "umnlib/localfeed",
  "description": "A PHP package for processing local Atom/RSS files, as opposed to remote files over HTTP.",
  "keywords": [],
  "authors": [
    {
      "name": "David Naughton",
      "email": "naughton@umn.edu"
    }
  ],

  "require": {
    "umnlib/set": "1.0.*",
    "umnlib/file-set-datesequence": "1.0.*",
    "simplepie/simplepie": "1.3.1",
    "symfony/finder": "v2.5.2"
  },

  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:UMNLibraries/set-php.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:UMNLibraries/file-set-datesequence-php.git"
    }
  ],   

  "require-dev": {
    "phpunit/phpunit": ">=3.7"
  },

  "autoload": {
    "psr-4": {"UmnLib\\Core\\LocalFeed\\": "src/"}
  },

  "autoload-dev": {
    "psr-4": {"UmnLib\\Core\\Tests\\": "tests/"}
  }
}
